{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "config": {
    "includeFiles": [
      "dist/**",
      "prisma/**",
      "node_modules/.prisma/client/**"
    ]
  }
}